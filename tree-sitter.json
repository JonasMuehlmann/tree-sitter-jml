{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jml",
      "camelcase": "JML",
      "title": "Java Modeling Language",
      "scope": "source.jml",
      "file-types": [
        "java"
      ],
      "injection-regex": "^jml$",
      "class-name": "TreeSitterJml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for the Java Modeling Language (JML)",
    "authors": [
      {
        "name": "Jonas MÃ¼hlmann",
        "email": "49788305+JonasMuehlmann@users.noreply.github.com"
      }
    ],
    "links": {
      "repository": "https://github.com/JonasMuehlmann/tree-sitter-jml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}